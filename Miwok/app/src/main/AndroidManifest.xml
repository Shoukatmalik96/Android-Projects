<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoukatmalik.miwok" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumbersActivity" />

        <intent-filter>
            <action android:name="com.example.shoukatmalik.miwok.NumbersActivity" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        <activity android:name=".FamilyMembers" />
        <intent-filter>
            <action android:name="com.example.shoukatmalik.miwok.FamilyMembers" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        <activity android:name=".Colors" />
        <intent-filter>
            <action android:name="com.example.shoukatmalik.miwok.Colors" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        <activity android:name=".Phrases" >
            <intent-filter>
                <action android:name="com.example.shoukatmalik.miwok.Phrases" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>